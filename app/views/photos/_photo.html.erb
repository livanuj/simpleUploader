<div class="media">
  <div class="media-left">
    <%#= link_to image_tag(photo.image.url, class: 'media-object'), photo.image.url, target: '_blank' %>
    <%= link_to image_tag(photo.image.url(:thumb), class: 'media-object'),{:action => 'project_content', :id => photo.id,:username=>photo.author} %>
    <%#= video_player({:file => photo.image.url}) %>
  </div>
  <div class="media-body">
    <h4 class="media-heading"><%= photo.title%></h4>
  </div>
</div>